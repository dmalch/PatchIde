<idea-plugin version="2">
    <name>Patch Ide</name>

    <id>PatchIde</id>

    <description>Patches IDEA</description>

    <change-notes>Initial version</change-notes>

    <version>1.0</version>

    <idea-version since-build="107.105"/>

    <vendor url="https://github.com/dmalch/ColorIde">Dmitry Malchikov</vendor>

    <depends>com.intellij.modules.lang</depends>

    <application-components>
        <component>
            <implementation-class>com.github.dmalch.PatchIdeApplicationComponent</implementation-class>
        </component>
    </application-components>

    <extensions defaultExtensionNs="com.intellij">
        <applicationConfigurable instance="com.github.dmalch.PatchIdeSystemSettings"/>
    </extensions>
</idea-plugin>